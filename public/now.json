{
    "version": 2,
    "name": "ascend-app",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ],
    "build": {
      "env": {
          "SERVER_ENDPOINT": "https://ascend-app-server.herokuapp.com/"
      }
  }
  }