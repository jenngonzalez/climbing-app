{
    "version": 2,
    "name": "ascend",
    "routes": [
      {
       "src": "^/static/(.*)",
       "dest": "/static/$1"
      },
      {
       "src": "^/favicon.ico$",
       "dest": "/favicon.ico"
      },
      {
       "src": "^/manifest.json$",
       "dest": "/manifest.json"
      },
      {
       "src": ".*",
       "dest": "/index.html"
      }
    ],
    "build": {
      "env": {
          "SERVER_ENDPOINT": "https://ascend-app-server.herokuapp.com/"
      }
  }
  }